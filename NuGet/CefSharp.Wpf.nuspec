      <frameworkAssembly assemblyName="System.Windows.Controls" targetFramework="sl50" />
      <frameworkAssembly assemblyName="System.Windows.Controls.Navigation" targetFramework="sl50" />
    </frameworkAssemblies>
  </metadata>
  <files>
