    <!-- CEF/Chromium files -->
    <file src="..\Release\avcodec-54.dll" target="cef"/>
    <file src="..\Release\avformat-54.dll" target="cef"/>
    <file src="..\Release\avutil-51.dll" target="cef"/>
    <file src="..\Release\icudt.dll" target="cef"/>
    <file src="..\Release\libcef.dll" target="cef"/>
    <file src="..\Release\libcef.dll" target="cef"/>
    <file src="..\Release\libEGL.dll" target="cef"/>
    <file src="..\Release\libGLESv2.dll" target="cef"/>
    <file src="..\Release\devtools_resources.pak" target="cef\devtools_resources.pak"/>

    <!-- Only including en-US for now -->
    <file src="..\Release\locales\en-US.pak" target="cef\locales"/>
